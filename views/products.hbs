<div class="row">
   <div class"col-md-12">
    <h1>End Point de productos</h1>
    <h2>{{title}}</h2>
   <a href="./newProduct" class="btn btn-primary btn-lg active" role="button">Add New</a>
  </div>
</div> 
<br>
 <div class="row">
   {{#each products}}
     <div class="col-md-4">
       <div class="card">
         <div class="img-card">
          <img src="{{this.photo}}" class="img-responsive">
         </div>
         <div class="text">
           <h3>{{this.name}}</h3>
           <p>Stock: {{this.stock}}</p>
         </div>
       </div>
       </br>
     </div> 
    {{/each}}
</div>
